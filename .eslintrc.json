{
	"env": {
		"browser": true,
		"commonjs": false,
		"es6": true,
		"node": true
	},
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": false
		},
		"sourceType": "module"
	},
	"rules": {
		"no-const-assign": "warn",
		"no-this-before-super": "warn",
		"no-undef": "error",
		"no-unreachable": "warn",
		"no-unused-vars": ["warn", {"args": "all", "argsIgnorePattern": "^_"}],
		"constructor-super": "warn",
		"valid-typeof": "warn",
		"semi": "warn"
	},
	"globals": {
		"ApplePaySession": true,
		"ChargeDesk": true
	}
}